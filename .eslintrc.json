{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "document": true,
    "window": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-console": "warn"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src", "src/app"]
      }
    }
  }
}
